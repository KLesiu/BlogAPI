{
    "version":2,
    "builds":[
        {
        "src":"./index.js",
        "use":"@vercel/node"
}
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
    "functions": {
        "api/*.js": {
          
          "maxDuration": 10 // This function can run for a maximum of 10 seconds
        }
      }
}